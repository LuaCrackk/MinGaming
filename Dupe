local minimized = false
local PlusButton = Instance.new("TextButton", ScreenGui)
PlusButton.Size = UDim2.new(0,30,0,30)
PlusButton.Position = UDim2.new(0,10,0,200)
PlusButton.Text = "M·ªü"
