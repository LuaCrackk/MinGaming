PlusButton.Visible = false

ToggleButton.MouseButton1Click:Connect(function()
    minimized = true
    Frame.Visible = false
    PlusButton.Visible = true
end)

PlusButton.MouseButton1Click:Connect(function()
    minimized = false
    Frame.Visible = true
    PlusButton.Visible = false
end)
